{
    "name": "letsgoi/laravel-settings",
    "description": "Laravel settings package",
    "keywords": [
        "goi",
        "letsgoi",
        "laravel",
        "settings"
    ],
    "homepage": "https://github.com/letsgoi/laravel-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "GOI",
            "email": "tech@letsgoi.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/http": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "letsgoi/php-code-style": "^1.2",
        "orchestra/testbench": "^7.19"
    },
    "autoload": {
        "psr-4": {
            "Letsgoi\\LaravelSettings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Letsgoi\\LaravelSettings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "syntax": "phpcs"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
